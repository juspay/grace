{
  "name": "@mass-research/cli",
  "version": "1.0.0",
  "description": "Professional AI-powered deep web research CLI and library with beautiful terminal UI",
  "main": "dist/lib/lib/index.js",
  "types": "dist/lib/lib/index.d.ts",
  "bin": {
    "mass-research": "dist/bin/bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/lib/lib/index.d.ts",
      "require": "./dist/lib/lib/index.js"
    },
    "./cli": {
      "types": "./dist/bin/bin/cli.d.ts",
      "default": "./dist/bin/bin/cli.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "custom_instructions.txt",
    "setup.js",
    "searxng-config.yml"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:lib && npm run build:cli",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:cli": "tsc -p tsconfig.cli.json",
    "build:watch": "npm run build:lib -- --watch",
    "dev": "tsx src/bin/cli.ts",
    "start": "node dist/bin/bin/cli.js",
    "setup": "node setup.js",
    "setup:dev": "node setup.js && npm run dev",
    "clean": "rimraf dist bundle",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "bundle": "npm run build && webpack",
    "bundle:watch": "npm run build && webpack --watch",
    "start:bundle": "node bundle/mass-cli.js",
    "docs": "typedoc src/lib/index.ts --out docs",
    "prepublishOnly": "npm run build && npm run test && npm run lint",
    "release": "npm run prepublishOnly && npm publish",
    "release:beta": "npm run prepublishOnly && npm publish --tag beta"
  },
  "keywords": [
    "ai",
    "research",
    "web-scraping",
    "cli",
    "typescript",
    "terminal-ui",
    "deep-research",
    "web-crawler",
    "ai-analysis",
    "blessed",
    "litellm",
    "anthropic",
    "openai"
  ],
  "author": {
    "name": "MASS Research Team",
    "email": "research@mass.ai",
    "url": "https://github.com/mass-research"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mass-research/cli.git"
  },
  "bugs": {
    "url": "https://github.com/mass-research/cli/issues"
  },
  "homepage": "https://github.com/mass-research/cli#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "playwright": "^1.55.1",
    "fs-extra": "^11.2.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "node-fetch": "^3.3.2",
    "uuid": "^10.0.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.3",
    "keypress": "^0.2.1",
    "terminal-kit": "^3.0.1",
    "terminal-image": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/uuid": "^9.0.8",
    "@types/blessed": "^0.1.25",
    "@types/cli-progress": "^3.11.5",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "node-loader": "^2.0.0"
  },
  "peerDependencies": {
    "playwright": ">=1.40.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
